#include<bits/stdc++.h>
using namespace std;

void printVec(vector<int>v){   // printing the elements of the vector
for(int i=0;i<v.size();i++){
    cout<<v[i];
}
cout<<endl;
}



// main function

int main(){
vector<int>v; // 
cout<<"enter the number"<<endl;
int n;  // no. of elements required
cin>>n;

cout<<"enter the value for the elements"<<endl;  // here  x taking the value
for(int i=0;i<n;i++){
    int x;  // in 'x' we access the element value
    cin>>x;
    v.push_back(x);   // push_back() inserts new element at the end of vector and increases size of vector by one.
    
}
printVec(v);  // call the printVec to print the elements
}
