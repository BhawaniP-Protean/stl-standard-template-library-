#include<bits/stdc++.h>
using namespace std;

void printVec(vector<int>v){   //function for print
for(int i=0;i<v.size();i++){
    cout<<v[i]<<" ";
}
cout<<endl;
}

int main(){
    vector<int>v;   
    cout<<"enter the number"<<endl;
    int n;
    cin>>n;
    cout<<"enter the values"<<endl;
    for(int i=0;i<n;i++){
        int x;          
        cin>>x;     //values for element
        v.push_back(x);    // inserting the element in the array

    }
    v.pop_back();   // it will remove the the last element in the vector

    printVec(v);   //call the print function
}
