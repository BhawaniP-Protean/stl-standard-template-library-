#include<bits/stdc++.h>
using namespace std;
void PrintVec(vector<pair<int,int> >v){
for(int i=0;i<v.size();i++){
    cout<<v[i].first<<" "<<v[i].second<<endl;
}
cout<<endl;
}

int main(){
 vector<pair<int,int> >v;
PrintVec(v);
cout<<"enter the number";
int n;
cin>>n;
for(int i=0;i<n;i++){
    int x,y;   //entering the value of x and y
    cin>>x>>y;
    v.push_back({x,y});

}
PrintVec(v);
}
